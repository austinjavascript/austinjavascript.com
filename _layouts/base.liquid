  </svg>

  <header class="page-header hero">
    <div class="hero-body is-primary is-paddingless">
      <div class="container">
        <div class="navbar" role="navigation" aria-label="main navigation">
          <div class="navbar-brand">
            <figure class="image is-96x96 logo">
              <img src="{{ '/assets/icon/logo.svg' | url }}" alt="logo">
            </figure>
            <a href="{{ '/' | url }}" class="navbar-item title has-text-white is-size-1-tablet">
              {{ site.title }}
            </a>
          </div>
          <div class="navbar-menu is-size-4">
            <div class="navbar-end">
              <a class="navbar-item" href="{{ '/' | url }}">
                Home
              </a>
              <a class="navbar-item" href="{{ '/posts/meetups/' | url }}">
                Meetups
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>

    {%- if page.url == '/' -%}
      {% responsiveImage "./assets/img/carlos-alfonso-AlBgcDfDG_s-unsplash_min.jpg", "Austin Skyline by Carlos Alfonso on Unsplash" %}
    {%- endif -%}
  </header>

  <main class="page-main section has-background-white">
    <div class="container">
      {{ content }}

      <p class="edit-page content has-text-right-desktop">
        <a href="{{ site.repo }}/blob/master/{{ page.inputPath }}">Edit this page</a>
      </p>
    </div>
  </main>

  <footer class="page-footer footer mt-4 pt-4">
    <div class="container">
      <div class="navbar">
        <div class="navbar-menu is-active">
          <div class="navbar-start">
            <a class="navbar-item" href="{{ '/' | url }}">
              &copy; {{ app.now | date: "%Y" }} Austin JavaScript
            </a>
            <a class="navbar-item" href="{{ '/about/' | url }}">
              About
            </a>
            <a class="navbar-item" href="{{ '/code-of-conduct/' | url }}">
              Code of Conduct
            </a>
            <a class="navbar-item" href="{{ '/contributing/' | url }}">
              Contributing
            </a>
            <a class="navbar-item" href="{{ '/posts/' | url }}">
              Posts
            </a>
            <a class="navbar-item is-hidden-desktop" href="{{ '/posts/meetups/' | url }}">
              Meetups
            </a>
          </div>
          <div class="navbar-end">
            <a class="navbar-item is-inline-block-tablet" href="https://twitter.com/{{ site.author.twitter }}">
              <span class="icon tooltip" role="link" aria-label="@{{ site.author.twitter }} on Twitter">
                <svg class="icon-item" viewBox="0 0 512 512"><use xlink:href="#logo-twitter"></use></svg>
              </span>
              <span class="is-sr-only-desktop">Twitter: @{{ site.author.twitter }}</span>
            </a>
            <a class="navbar-item is-inline-block-tablet" href="{{ site.author.discord }}">
              <span class="icon tooltip" role="link" aria-label="{{ site.author.discord }} on Discord">
                <svg class="icon-item" viewBox="0 0 127.14 96.36"><use xlink:href="#logo-discord"></use></svg>
              </span>
              <span class="is-hidden-desktop">Discord: {{ site.author.discord }}</span>
            </a>
            <a class="navbar-item is-inline-block-tablet" href="https://github.com/{{ site.author.github }}">
              <span class="icon tooltip" role="link" aria-label="{{ site.author.github }} on GitHub">
                <svg class="icon-item" viewBox="0 0 512 512"><use xlink:href="#logo-github"></use></svg>
              </span>
              <span class="is-hidden-desktop">GitHub: {{ site.author.github }}</span>
            </a>
          </div>
        </div>
      </div>
    </div>
  </footer>
  {%- if app.environment != 'dev' -%}
  {%- include 'ga.html' -%}
  {%- endif -%}
</body>
</html>
